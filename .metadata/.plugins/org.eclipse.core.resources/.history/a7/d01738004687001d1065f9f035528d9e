/*Sick CSS Animations*/

@keyframes fadeIn {
	0% { opacity: 0;}
	100% { opacity: 1;}
  }

  @keyframes fadeUp {
	0% { opacity: 0; 
		transform: translate(0, 10vh);}
	100% { opacity: 1;
		transform: translate(0, 0); }
  }

  .tag {
	opacity: 0;
	transform: translate(0, 15vh);
	transition: all 1s;
  }

  .tag.visible {
	opacity: 1;
	transform: translate(0, 0);
  }


/*End of Sick Animations*/

